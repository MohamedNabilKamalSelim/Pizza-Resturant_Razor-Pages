@page
@model Pizzeria.Pages.OrdersModel
@{
}

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>Pizza Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var order in Model.orders){
            <tr>
                <td>@order.Id</td>
                <td>@order.PizzaName</td>
                <td>@order.Price</td>
            </tr>
        }
    </tbody>
</table>
